{
    "name": "@noahvarghese/react-components",
    "version": "0.1.10",
    "repository": "git+https://github.com/noahvarghese/react-components.git",
    "bugs": {
        "url": "https://github.com/noahvarghese/react-components/issues"
    },
    "homepage": "https://github.com/noahvarghese/react-components#readme",
    "scripts": {
        "start": "react-scripts start",
        "build-declaration": "./node_modules/.bin/tsc -p .",
        "build": "cross-env BABEL_ENV=production babel --extensions '.tsx' src -d dist",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "storybook": "start-storybook -p 6006 -s public",
        "build-storybook": "build-storybook -s public",
        "prepublish": "yarn build-declaration",
        "release": "auto shipit --base-branch=main"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "overrides": [
            {
                "files": [
                    "**/*.stories.*"
                ],
                "rules": {
                    "import/no-anonymous-default-export": "off"
                }
            }
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^11.1.0",
        "@testing-library/user-event": "^12.1.10",
        "@types/jest": "^26.0.15",
        "@types/node": "^12.0.0",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-router-dom": "^5.1.8",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.3",
        "sass": "^1.37.5",
        "tsc": "^2.0.3",
        "typescript": "^4.1.2",
        "web-vitals": "^1.0.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.14.8",
        "@storybook/addon-actions": "^6.3.6",
        "@storybook/addon-essentials": "^6.3.6",
        "@storybook/addon-links": "^6.3.6",
        "@storybook/node-logger": "^6.3.6",
        "@storybook/preset-create-react-app": "^3.2.0",
        "@storybook/react": "^6.3.6",
        "auto": "^10.30.0",
        "cross-env": "^7.0.3"
    },
    "files": [
        "./src/types/global.d.ts"
    ],
    "babel": {
        "presets": [
            [
                "react-app",
                {
                    "absoluteRuntime": false
                }
            ]
        ]
    },
    "description": "React components",
    "main": "dist/index.js",
    "author": "noahvarghese <varghese.noah@gmail.com>",
    "license": "MIT",
    "private": false
}
